{
    "version": 2,
    "public": false,
    "builds": [{ "src": "app.py", "use": "@now/python" }],
    "routes": [
        { "src": "/", "dest": "app.py" },
        { "src": "/troll", "dest": "app.py"},
        { "src": "/items/(.*)", "dest": "app.py"},
        { "src": "/example/(.*)", "dest": "app.py" },
        { "src": "/docs", "dest": "app.py" },
        { "src": "/troll-api", "dest": "app.py" },
        { "src": "/openapi.json", "dest": "app.py" }
    ]
}